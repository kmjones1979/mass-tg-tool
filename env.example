# Telegram API credentials (get from https://my.telegram.org/apps)
API_ID=your_api_id_here
API_HASH=your_api_hash_here

# Your phone number (with country code, e.g., +1234567890)
PHONE_NUMBER=+1234567890

# Optional: Session string (to avoid re-authentication)
# Get this from running 'npm run setup'
SESSION_STRING=your_session_string_here

# Session storage filename (optional, defaults to 'session')
SESSION_NAME=session

# Rate limiting (messages per second, defaults to 1)
# Lower values = safer, higher values = faster but risky
RATE_LIMIT=1 